{
  "expo": {
    "name": "Garista Waiter",
    "slug": "garistawaiter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "iosDisplayInForeground": true
     },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.garistawaiter",
      "googleServicesFile" : "./android/app/google-services.json"
    },
    "android": {
      "permissions": ["NOTIFICATIONS"],
      "package": "com.garistawaiter",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile" : "./android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#fff",
          "sounds": [
            "./assets/sounds/app_voice.wav"
          ]
        }
      ],
        "expo-build-properties"

    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "18169b21-e411-434a-87ee-6f7602199f61"
      }
    },
    "owner": "ayoub802",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/18169b21-e411-434a-87ee-6f7602199f61"
    }
  }
}
